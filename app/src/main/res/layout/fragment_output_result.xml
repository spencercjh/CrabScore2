<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <RelativeLayout
            android:id="@+id/re_generate_score"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/btn_list_item_bg"
            android:clickable="true"
            android:focusable="true">
        <TextView
                android:id="@+id/tv_generate_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:text="计算成绩"
                android:textColor="#353535"
                android:textSize="16sp"/>
    </RelativeLayout>

    <RelativeLayout
            android:id="@+id/re_output_excel"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/btn_list_item_bg"
            android:clickable="true"
            android:focusable="true">
        <TextView
                android:id="@+id/tv_output_excel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:text="导出excel"
                android:textColor="#353535"
                android:textSize="16sp"/>
    </RelativeLayout>

    <RelativeLayout
            android:id="@+id/re_update_present_year"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@drawable/btn_list_item_bg"
            android:clickable="true"
            android:focusable="true">
        <TextView
                android:id="@+id/tv_update_present_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="20dp"
                android:text="修改当前大赛"
                android:textColor="#353535"
                android:textSize="16sp"/>
    </RelativeLayout>
</LinearLayout>